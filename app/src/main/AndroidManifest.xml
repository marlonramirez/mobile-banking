<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.edu.usbcali.mobilebanking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".view.ProductListActivity"></activity>
        <activity android:name=".view.ProductDetailActivity"></activity>
        <activity android:name=".view.ServiceListActivity"></activity>
        <activity android:name=".view.ServiceActivity"></activity>
        <activity android:name=".view.AccountListActivity"></activity>
        <activity android:name=".view.AccountActivity"></activity>
        <activity android:name=".view.PaymentActivity"></activity>
        <activity android:name=".view.TransferActivity"></activity>
    </application>

</manifest>